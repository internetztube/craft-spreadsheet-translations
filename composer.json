{
    "name": "internetztube/craft-spreadsheet-translations",
    "description": "Adds support for dynamic translations from Google Spreadsheets.",
    "type": "craft-plugin",
    "version": "2.0.0-beta.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "spreadsheet translations",
        "translation",
        "Google Spreadsheets"
    ],
    "support": {
        "docs": "https://github.com/internetztube/craft-spreadsheet-translations/blob/master/README.md",
        "issues": "https://github.com/internetztube/craft-spreadsheet-translations/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Frederic KÃ¶berl",
            "homepage": "https://github.com/internetztube/"
        }
    ],
    "require": {
        "php": "^8.0",
        "craftcms/cms": "^4.0.0-alpha",
        "google/apiclient": "^2.4"
    },
    "autoload": {
        "psr-4": {
          "internetztube\\spreadsheetTranslations\\": "src/"
        }
    },
    "extra": {
        "name": "Spreadsheet Translations",
        "handle": "spreadsheet-translations",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/internetztube/craft-spreadsheet-translations/master/CHANGELOG.md",
        "class": "internetztube\\spreadsheetTranslations\\SpreadsheetTranslations",
        "documentationUrl": "https://github.com/internetztube/craft-spreadsheet-translations/blob/master/README.md"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
